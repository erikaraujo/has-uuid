    "keywords": [
        "erikaraujo",
        "has-uuid",
        "laravel-has-uuid",
        "laravel-uuid",
        "laravel-uuid-trait",
        "uuid-trait",
        "uuid"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "ErikAraujo\\HasUuid\\": "src"
        }
    },
    "autoload-dev": {
